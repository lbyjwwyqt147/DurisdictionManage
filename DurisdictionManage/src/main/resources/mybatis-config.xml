    <!-- mapper 文件路径配置 -->  
    <mappers>  
        <mapper resource="sqlMapperXml/UserMapper.xml" />  
        <mapper resource="sqlMapperXml/AttachmentMapper.xml" />  
    </mappers>  
</configuration>  
