"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>  
    <!-- 实体类别名配置 -->  
    <typeAliases>
       <typeAlias type="pers.liujunyi.tally.entity.TCoreUser" alias="CoreUser"/>
       <typeAlias type="pers.liujunyi.tally.entity.TCoreAttachment" alias="CoreAttachment"/>
